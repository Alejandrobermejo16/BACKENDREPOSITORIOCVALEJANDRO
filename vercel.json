{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "api/users.js", "use": "@vercel/node" },
    { "src": "api/createUser.js", "use": "@vercel/node" },
    { "src": "api/getUserById.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/users", "dest": "api/users.js", "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"], "middlewares": ["_middleware"] },
    { "src": "/api/users/create", "dest": "api/createUser.js", "methods": ["POST", "OPTIONS"], "middlewares": ["_middleware"] },
    { "src": "/api/users/:userId", "dest": "api/getUserById.js", "methods": ["GET", "PUT", "DELETE", "OPTIONS"], "middlewares": ["_middleware"] },
    { "src": "/(.*)", "dest": "index.js", "middlewares": ["_middleware"] }
  ]
}

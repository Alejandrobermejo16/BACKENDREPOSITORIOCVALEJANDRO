{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/createUser.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/users/create",
      "dest": "/api/createUser.js"
    }
  ]
}
 
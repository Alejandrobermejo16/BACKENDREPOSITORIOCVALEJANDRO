{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/createUser.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/logguin.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/calUser.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cron/updateCalories.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/users/create",
      "dest": "/api/createUser.js"
    },
    {
      "src": "/api/users/logguin",
      "dest": "/api/logguin.js"
    },
    {
      "src": "/api/users/cal",
      "dest": "/api/calUser.js"
    },
    {
      "src": "/api/cron/update-calories",
      "dest": "/api/cron/updateCalories.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/update-calories",
      "schedule": "* * * * *"  
    }
  ]
}
